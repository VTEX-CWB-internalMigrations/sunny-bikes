{
    "store.custom#institutionalCustom": {
        "blocks": [
            "flex-layout.row#institutional"
        ]
    },
    "flex-layout.row#institutional": {
        "children": [
            "flex-layout.col#institutional"
        ],
        "props": {
            "blockClass": "institutionalContainer"
        }
    },
    "flex-layout.col#institutional": {
        "children": [
            "rich-text#institutionalText"
        ],
        "props": {
            "blockclass": "textColumn",
            "preventVerticalStretch": true
        }
    },
    "rich-text#institutionalText": {
        "props": {
            "text": "Seu texto institucional aqui",
            "blockClass": "instutitionalText"
        }
    }
}